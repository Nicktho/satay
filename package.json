{
  "name": "satay",
  "version": "1.1.1",
  "description": "A utility for uploading static sites to AWS S3.",
  "keywords": [
    "aws",
    "s3",
    "bucket",
    "website",
    "static-site",
    "deploy",
    "upload"
  ],
  "repository": "jameslnewell/satay",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "aws-sdk": "^2.48.0",
    "chalk": "^1.1.3",
    "dotenv": "^4.0.0",
    "glob": "^7.1.1",
    "lodash.difference": "^4.5.0",
    "lodash.omit": "^4.5.0",
    "mime-types": "^2.1.15",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "flow-bin": "^0.45.0",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "lint-staged": "^3.4.1",
    "prettier": "^1.3.1"
  },
  "bin": {
    "satay": "lib/cli.js"
  },
  "scripts": {
    "flow": "flow",
    "build": "babel ./src --out-dir ./lib --ignore test.js",
    "test": "jest",
    "example": "cd example && node ../lib/cli.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote --no-bracket-spacing",
      "git add"
    ]
  },
  "license": "MIT"
}
